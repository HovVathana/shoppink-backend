{
  "name": "shoppink-backend",
  "version": "1.0.0",
  "description": "Backend for ShopPink e-commerce admin system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npx prisma migrate deploy && npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "node prisma/seed.js",
    "seed:orders": "node scripts/seedOrders.js",
    "vercel-build": "npx prisma migrate deploy && npx prisma generate",
    "postdeploy": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0"
  },
  "postinstall": "prisma generate",
  "keywords": [
    "express",
    "nodejs",
    "ecommerce",
    "api"
  ],
  "author": "",
  "license": "MIT"
}
